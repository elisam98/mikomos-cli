<table class="table table-hover">
	<tbody>
	{{#each model}}
		{{#link-to 'makom' id tagName="tr" classNames="pointer"}}
		<td>{{#each categories}}<span class="flaticon flaticon-{{this}} flaticon-2x"></span>{{/each}}</td>
		<td>{{title}}</td>
		<td>{{address}}</td>
		<td>{{city}}</td>
		<td>{{state}}</td>
		<td>{{zip}}</td>
		<td>{{phone}}</td>
		{{/link-to}}
	{{/each}}
	</tbody>
</table>